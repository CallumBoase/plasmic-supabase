{
  "name": "plasmic-supabase",
  "version": "0.5.0-alpha-15",
  "description": "Code components to use Supabase as a backend for a Plasmic app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "build": "tsup-node src/index.tsx --dts --format esm,cjs --target es2019",
    "test": "TEST_CWD=`pwd` yarn --cwd=../.. test --passWithNoTests",
    "prepublishOnly": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/cookie": "^0.6.0",
    "@types/dlv": "^1.1.2",
    "@types/node": "^17.0.14",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "size-limit": "^7.0.8",
    "tslib": "^2.3.1",
    "tsup": "^7.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "use-deep-compare": "^1.2.1",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@plasmicapp/host": "^1.0.208",
    "@plasmicapp/query": "^0.1.79",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.10",
    "@uppy/dashboard": "^3.7.5",
    "@uppy/drag-drop": "^3.0.3",
    "@uppy/file-input": "^3.1.0",
    "@uppy/progress-bar": "^3.1.0",
    "@uppy/react": "^3.2.2",
    "@uppy/tus": "^3.5.3",
    "next": "^14.2.22",
    "react": "^18",
    "react-dom": "^18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CallumBoase/plasmic-supabase.git"
  },
  "author": "Callum Boase, Ryan Mouritz",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CallumBoase/plasmic-supabase/issues"
  },
  "homepage": "https://github.com/CallumBoase/plasmic-supabase#readme"
}
